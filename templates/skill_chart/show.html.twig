{% extends 'admin/layout.html.twig' %}

{% block title %}SkillChart{% endblock %}

{% block body %}
    <h1>SkillChart</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ skill_chart.id }}</td>
            </tr>
            <tr>
                <th>Techno</th>
                <td>{{ skill_chart.techno }}</td>
            </tr>
            <tr>
                <th>Value</th>
                <td>{{ skill_chart.value }}</td>
            </tr>
        </tbody>
    </table>

    <div class="my-5">
        <a href="{{ path('app_skill_chart_index') }}" class="p-2 bg-color-brown">back to list</a>
        <a href="{{ path('app_skill_chart_edit', {'id': skill_chart.id}) }}" class="p-2 bg-color-light-brown">edit</a>
        {{ include('skill_chart/_delete_form.html.twig') }}
    </div>
{% endblock %}
